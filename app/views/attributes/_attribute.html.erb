<tr id="<%= dom_id attribute %>">
  <td><%= attribute.name %></td>
  <td><%= attribute.numerical %></td>
  <td><%= image_tag(attribute.icon, style: "width: 5%") if attribute.icon.attached?%></td>
  <td><%= link_to 'Show', attribute_path(attribute), {:remote => true, 'data-toggle' =>  "modal", 'data-target' => '#modal-window-show'} %></td>
  <td><%= link_to 'Edit', edit_attribute_path(attribute, :type_id => attribute.type_id), {:remote => true, 'data-toggle' =>  "modal", 'data-target' => '#modal-window-show'} %></td>
  <td><%= link_to 'Destroy', attribute, method: :delete, data: { remote: true }, class: "delete" %></td>
</tr>

